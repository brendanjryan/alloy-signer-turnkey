# Turnkey Organization ID (get from Turnkey dashboard)
TURNKEY_ORGANIZATION_ID=your-organization-id

# Turnkey API Private Key (get from Turnkey dashboard)
TURNKEY_API_PRIVATE_KEY=your-api-private-key-hex

# Either use a private key ID OR a wallet ID (not both)

# Option 1: Private Key ID (for direct private key signing)
TURNKEY_PRIVATE_KEY_ID=your-private-key-id

# Option 2: Wallet ID (for wallet-based signing)
# TURNKEY_WALLET_ID=your-wallet-id

# The Ethereum address controlled by your private key/wallet
ETHEREUM_ADDRESS=0xfoo
